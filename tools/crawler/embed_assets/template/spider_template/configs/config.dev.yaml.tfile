services:
  crawler:
    spider:
      # 爬虫名
      Name: '{@spider_name}'
      # 提交初始化种子的时机
      SubmitInitialSeedOpportunity: 'start'
      # 默认请求方法
      #RequestMethod: 'get'
      # 允许使用代理
      #AllowProxy: true
      # user-agent 类型; pc,android,ios
      #UserAgentType: 'pc'
      # 是否自动管理cookie, 当前任务提交的种子会继承之前的cookies
      #AutoCookie: false
      # 是否自动跳转
      #AutoRedirects: true
      # 默认html编码
      #HtmlEncoding: 'utf8'
      # 期望的http状态码列表, 示例: [200, 204]
      #ExpectHttpStatusCode: []
      # 无效的http状态码列表, 如果配置了ExpectHttpStatusCode, 则以ExpectHttpStatusCode为准. 示例: [404, 500]
      #InvalidHttpStatusCode: []
      # 将http状态码4xx视为无效, spider会自动重试
      #HttpStatus4xxIsInvalid: true
      # 将http状态码5xx视为无效, spider会自动重试
      #HttpStatus5xxIsInvalid: true
      # 对某些 ContentType 自动去掉utf8bom头, 多种类型用英文逗号分隔
      #AutoTrimUtf8BomWithContentType: text/html
